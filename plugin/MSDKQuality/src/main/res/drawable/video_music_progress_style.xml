<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ff2eb0f0" android:endColor="#ff2eb0f0" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ffff8900" android:endColor="#ffff8900" android:angle="270.0" android:centerColor="#ffff8900" />
            </shape>
        </clip>
    </item>
</layer-list>